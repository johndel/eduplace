<p id="notice"><%= notice %></p>

<h1>Edutypes</h1>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @edutypes.each do |edutype| %>
      <tr>
        <td><%= edutype.name %></td>
        <td><%= link_to 'Show', edutype %></td>
        <td><%= link_to 'Edit', edit_edutype_path(edutype) %></td>
        <td><%= link_to 'Destroy', edutype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>