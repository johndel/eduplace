<p id="notice"><%= notice %></p>

<h1>Places</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Title</th>
      <th>Site</th>
      <th>Address</th>
      <th>Postal code</th>
      <th>Phone</th>
      <th>Fax</th>
      <th>Permission code</th>
      <th>Edutype</th>
      <th>Amea</th>
      <th>Trainee capacity</th>
      <th>City</th>
      <th>County</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @places.each do |place| %>
      <tr>
        <td><%= place.name %></td>
        <td><%= place.title %></td>
        <td><%= place.site %></td>
        <td><%= place.address %></td>
        <td><%= place.postal_code %></td>
        <td><%= place.phone %></td>
        <td><%= place.fax %></td>
        <td><%= place.permission_code %></td>
        <td><%= place.edutype %></td>
        <td><%= place.amea %></td>
        <td><%= place.trainee_capacity %></td>
        <td><%= place.city %></td>
        <td><%= place.county %></td>
        <td><%= link_to 'Show', place %></td>
        <td><%= link_to 'Edit', edit_place_path(place) %></td>
        <td><%= link_to 'Destroy', place, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Place', new_place_path %>
