<%= form_for(place, html: { class: "form-inline" }) do |f| %>
  <% if place.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(place.errors.count, "error") %> prohibited this place from being saved:</h2>

      <ul>
      <% place.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>

  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title, class: "form-control" %>
  </div>

  <div class="field">
    <%= f.label :site %>
    <%= f.text_field :site, class: "form-control" %>
  </div>

  <div class="field">
    <%= f.label :address %>
    <%= f.text_field :address, class: "form-control" %>
  </div>

  <div class="field">
    <%= f.label :postal_code %>
    <%= f.number_field :postal_code, class: "form-control" %>
  </div>

  <div class="field">
    <%= f.label :phone %>
    <%= f.text_field :phone, class: "form-control" %>
  </div>

  <div class="field">
    <%= f.label :fax %>
    <%= f.text_field :fax, class: "form-control" %>
  </div>

  <div class="field">
    <%= f.label :permission_code %>
    <%= f.number_field :permission_code, class: "form-control" %>
  </div>

  <div class="field">
    <%= f.label :edutype %>
    <%= f.number_field :edutype, class: "form-control" %>
  </div>

  <div class="field">
    <%= f.label :amea %>
    <%= f.check_box :amea, class: "form-control" %>
  </div>

  <div class="field">
    <%= f.label :trainee_capacity %>
